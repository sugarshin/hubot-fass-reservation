{
  "name": "hubot-fass-reservation",
  "description": "A Hubot script for utilities of FaSS reservation",
  "version": "0.0.8",
  "author": "Shingo Sato <shinsugar@gmail.com>",
  "main": "index.coffee",
  "license": "MIT",
  "keywords": [
    "hubot",
    "hubot-scripts"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/sugarshin/hubot-fass-reservation.git"
  },
  "bugs": {
    "url": "https://github.com/sugarshin/hubot-fass-reservation/issues"
  },
  "homepage": "https://github.com/sugarshin/hubot-fass-reservation#readme",
  "scripts": {
    "test": "grunt test",
    "update-pkg-version": "npm --no-git-tag-version version",
    "postupdate-pkg-version": "git add package.json package-lock.json && MESSAGE=$(node -p \"require('./package.json').version\"); git commit -m $MESSAGE",
    "u": "npm run update-pkg-version",
    "u:patch": "npm run u -- patch",
    "u:minor": "npm run u -- minor",
    "u:major": "npm run u -- major"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "iconv-lite": "^0.5.0",
    "lodash": "^4.17.15",
    "ms": "^2.1.2",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coffeescript": "2.4.1",
    "grunt": "1.0.4",
    "grunt-cli": "1.3.2",
    "grunt-contrib-watch": "1.1.0",
    "grunt-mocha-test": "0.13.3",
    "hubot": "2.19.0",
    "hubot-test-helper": "1.9.0",
    "matchdep": "2.0.0",
    "mocha": "5.2.0",
    "sinon": "6.3.5",
    "sinon-chai": "3.3.0"
  }
}
